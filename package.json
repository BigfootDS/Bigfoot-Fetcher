{
  "name": "@bigfootds/bigfoot-fetcher",
  "version": "0.2.0",
  "description": "JavaScript fetch override for cohesive, consistent usage across various BigfootDS projects.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "start": "node dist/index.js",
    "dev": "npm run build && npm run start",
    "build": "tsc",
    "envcheck:ps": "echo %npm_package_config_bigfootds_productName%",
    "envcheck:bash": "echo ${npm_package_config_bigfootds_productName}"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BigfootDS/Bigfoot-Fetcher.git"
  },
  "keywords": [],
  "author": {
    "name": "Alex Stormwood",
    "email": "alex@bigfootds.com",
    "url": "https://github.com/AlexStormwood"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BigfootDS/Bigfoot-Fetcher/issues"
  },
  "homepage": "https://github.com/BigfootDS/Bigfoot-Fetcher#readme",
  "engines": {
    "node": ">=20.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.1",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "bowser": "^2.11.0",
    "my-ua-parser": "^2.0.4"
  }
}
